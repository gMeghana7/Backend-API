{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "routes/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/books",
      "dest": "routes/bookRoutes.js"
    },
    {
      "src": "/api/users",
      "dest": "routes/userRoutes.js"
    }
  ]
}
